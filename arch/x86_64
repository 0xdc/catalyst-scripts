arch=amd64
targets=(
	systemd:stage1
	systemd:stage2
	systemd:stage3

	openstack:python
	minimal:nginx
	router:stage1
	systemd:stage4

	systemd:livecd-stage1
	systemd:livecd-stage2
	systemd:livecd-stage3

	#https://bugs.gentoo.org/688330
	sso:stage4

	minimal:embedded
	minimal:livecd-stage2
	minimal:livecd-stage3
)

case "$date" in
*01)
	targets+=(
		dev:stage4

		plasma:stage2
		plasma:stage3
		plasma:stage4

		plasma-sso:stage4

		browsers:stage4
	)
	;;
esac

upstream="$arch"
